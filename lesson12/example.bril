@f( a: int ): int {
  one: int = const 1;
  b: int = sub a one;
  ret b;
}
@g( a: int ): int {
  one: int = const 1;
  b: int = add a one;
  ret b;
}

@main(x: int) {
  one: int = const 1;
  hundred: int = const 100;
  y: int = add x one;
  cond: bool = lt x hundred;
  br cond .then .else;
.then:
  x: int = call @f x;
  jmp .done;
.else:
  x: int = call @g x;
  jmp .done;
.done:
  print x;
  ret;

}